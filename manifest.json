{
  "name": "BatEditor",
  "version": "0.1",
  "manifest_version": 2,

  "description": "Replaces textfield for code in CodingBat with a the powerful CodeMirror editor",

  "content_scripts": [
    {
      "matches": [
        "*://*.codingbat.com/prob/*"
      ],
      "js": ["lib/jquery-1.10.2.min.js", "lib/codemirror.js", "lib/matchbrackets.js", "lib/clike.js", "main.js"],
      "css": ["lib/codemirror.css"]
    }
  ]
}
